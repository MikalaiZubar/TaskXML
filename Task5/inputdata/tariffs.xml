   xsi:schemalocation="htt://www.mikalaizubar.com/tariffs tariff.xsd">
    <voice-tariff name="Easy_to_say" operator="MTS">
        <payroll>0.55</payroll>
        <call-prices>
            <internal-call>0.07</internal-call>
            <external-call>0.14</external-call>
            <international-call>1.38</international-call>
